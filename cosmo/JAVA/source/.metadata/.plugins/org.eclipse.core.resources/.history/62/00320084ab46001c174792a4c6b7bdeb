package sec02;

import java.util.Arrays;
import java.util.List;

public class FromCollectionExample {
	public static void main (String[] args) {
		List<Student> studentList = Arrays.asList(
				new Student("홍길동", 10),
				new Student("신용권", 10),
				new Student("유미선", 10)
				);
		Stream<Student> stream = studentList.stream();
		
	}
}
